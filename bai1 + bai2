//bai 1 
var inputArray = ["Quang", "Minh", "Linh", "Khanh" ]
function allLongestStrings(inputArray) {
    var name = inputArray.split('')
    var max = 0
    var word = null
    for (var i = 0; i < name.length; i++) {
        if (max < name[i].length) {
            max = name[i].length
            word = name[i]
        }
    }
    return word
}

